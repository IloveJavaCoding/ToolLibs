<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="48dp"
    android:height="48dp"
    android:viewportWidth="48.0"
    android:viewportHeight="48.0">

    <group
        android:name="circle"
        android:pivotX="24"
        android:pivotY="24">

        <group>
            <path
                android:strokeWidth="0.1"
                android:strokeColor="@color/colorGery_Half"
                android:pathData="M 14 24 a 10 10 0 1 0 34 0
                                  M 14 24 a 10 10 0 1 1 34 0"/>
        </group>

        <group
            android:translateX="30"
            android:translateY="6">
            <path
                android:fillColor="@color/colorGery_Half"
                android:pathData="M 1 1
                    m -1 0 a 0.5 0.5 0 1 0 2 0
                    m -2 0 a 0.5 0.5 0 1 1 2 0"/>
        </group>
    </group>
</vector>

