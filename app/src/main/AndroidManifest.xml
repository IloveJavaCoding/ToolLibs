<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toollibs">

    <application
        android:name=".Activity.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.UI.Activity_View_VideoView"></activity>
        <activity android:name=".Activity.UI.Activity_View_WebView" />
        <activity android:name=".Activity.UI.Activity_View_ImageView" />
        <activity android:name=".Activity.UI.Activity_Bar" />
        <activity android:name=".Activity.UI.Activity_Show_Data" />
        <activity android:name=".Activity.UI.Activity_Selector" />
        <activity android:name=".Activity.UI.Activity_View" />
        <activity android:name=".Activity.UI.Activity_Button" />
        <activity android:name=".Activity.UI.Activity_Text" />
        <activity android:name=".Activity.Alarm_Activity" />
        <activity android:name=".Activity.Time_Activity" />
        <activity android:name=".Activity.Urat5168_Activity">
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Radio_Check_Group_Activity" />
        <activity android:name=".Activity.SeekBar_Activity" />
        <activity android:name=".Activity.TimerSelector_Activity" />
        <activity android:name=".Activity.Fragment_Activity" />
        <activity android:name=".Activity.Dialog_Activity" />
        <activity android:name=".Activity.PosterView_Activity" />
        <activity android:name=".Activity.Spinner_Activity" />
        <activity android:name=".Activity.DelEditText_Activity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Activity.Receiver.MyBroadcastReceiver">
            <intent-filter>
                <action android:name="com.example.ToolLibs.execshellcommand" />
                <action android:name="com.example.ToolLibs.install" />
                <action android:name="com.example.ToolLibs.reboot" />
            </intent-filter>
        </receiver>
        <receiver android:name=".Activity.Receiver.AlarmReceiver">
            <intent-filter>
                <action android:name="com.example.ToolLibs.alarm" />
            </intent-filter>
        </receiver>
    </application>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.PERMISSIONS_STORAGE" />

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

</manifest>